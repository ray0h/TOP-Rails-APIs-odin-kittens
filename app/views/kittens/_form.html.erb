<%= form_with model: @kitten do |f| %>
  <%= f.label :name %><br/>
  <%= f.text_field :name, required: true %><br/>
  <%= f.label :age %><br/>
  <%= f.text_field :age, required: true %><br/>
  <%= f.label :cuteness %><br/>
  <%= f.text_field :cuteness, required: true %><br/>
  <%= f.label :softness %><br/>
  <%= f.text_field :softness, required: true %><br/>
  <%= f.submit %>
<% end %>