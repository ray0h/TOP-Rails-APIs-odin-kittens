<%= form_with model: @kitten do |f| %>
  <%= f.label :name %><br/>
  <%= f.text_field :name %><br/>
  <%= f.label :age %><br/>
  <%= f.text_field :age %><br/>
  <%= f.label :cuteness %><br/>
  <%= f.text_field :cuteness %><br/>
  <%= f.label :softness %><br/>
  <%= f.text_field :softness %><br/>
  <%= f.submit %>
<% end %>